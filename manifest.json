{
    "manifest_version": 2,
    "name": "tab-bound",
    "version": "1.0",
    "description": "Enforces a bound on the number of tabs.",
    "icons": {
        "32": "icons/tb32.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/tb32.png",
        "default_title": "tab-bound",
        "default_popup": "tab-bound.html"
    },
    "options_ui": {
        "page": "options/options.html"
    }
}